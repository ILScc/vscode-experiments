{
    "name": "zardoy-experiments",
    "displayName": "Zardoy Experiments",
    "description": "Highly experimental features and commands",
    "preview": true,
    "version": "0.0.0-dev",
    "publisher": "zardoy",
    "private": true,
    "license": "MIT",
    "activationEvents": [
        "onStartupFinished"
    ],
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
            {
                "command": "tab",
                "title": "Special Tab"
            },
            {
                "command": "openUrl",
                "title": "Open URL"
            },
            {
                "command": "addImport",
                "title": "Add Import"
            },
            {
                "command": "goToNextErrorInFile",
                "title": "Go to Error in File"
            },
            {
                "command": "fixedSurroundIf",
                "title": "fixed surround if"
            },
            {
                "command": "removeUnusedImports",
                "title": "remove unused imports"
            }
        ],
        "configuration": [
            {
                "properties": {
                    "features.preserveCamelCase": {
                        "type": "boolean",
                        "default": true
                    },
                    "features.tsCodeActions": {
                        "type": "boolean",
                        "default": true
                    },
                    "features.regexCodeActions": {
                        "type": "boolean",
                        "default": true
                    },
                    "features.missingVscodeImportCodeAction": {
                        "type": "boolean",
                        "default": true
                    }
                }
            }
        ]
    },
    "scripts": {
        "start": "vscode-framework start"
    },
    "dependencies": {
        "vscode-framework": "^0.0.18"
    },
    "devDependencies": {
        "@types/node": "^16.11.11",
        "@types/vscode": "^1.62.0",
        "@zardoy/tsconfig": "^1.2.2",
        "eslint": "^8.4.0",
        "eslint-config-zardoy": "^0.2.1",
        "typescript": "^4.5.2"
    }
}
